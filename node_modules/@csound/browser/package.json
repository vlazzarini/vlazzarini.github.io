{
  "name": "@csound/browser",
  "version": "6.18.0",
  "module": "./dist/csound.js",
  "typings": "index.d.ts",
  "type": "module",
  "files": [
    "dist/csound*",
    "dist/factory*",
    "polyfills",
    "script",
    "src",
    "tests/index.html",
    "tests/*.js",
    "index.d.ts"
  ],
  "license": "LGPL-2.1",
  "bugs": {
    "url": "https://github.com/csound/csound/issues"
  },
  "homepage": "https://github.com/csound/csound/tree/master/wasm/browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/csound/csound"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "@csound",
    "audimodules",
    "audio",
    "audio-plugin",
    "audio-plugins",
    "audiocontext",
    "audioworklet",
    "csound",
    "csound-wasm",
    "wasi",
    "wasm",
    "webassembly",
    "webaudio",
    "webmidi"
  ],
  "scripts": {
    "lint": "npx eslint 'src/**/*.js'",
    "lint:ci": "cross-env ESLINT_JUNIT_OUTPUT=tests/eslint.junit.xml npx eslint -f ./node_modules/eslint-junit/index.js 'src/**/*.js'",
    "build": "cross-env NODE_ENV=development node compile.js",
    "build:prod": "cross-env NODE_ENV=production node compile.js",
    "build:static": "cross-env BUILD_STATIC=1 yarn run build",
    "build:readme": "node --experimental-modules script/generate-readme.mjs > README.md",
    "test:ci": "node tests/selenium_runner.js",
    "test:ci:firefox": "node tests/selenium_runner_firefox.js",
    "test:open": "node tests/server.cjs"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-destructuring": "^7.16.7",
    "@csound/wasm-bin": "6.18.0",
    "browser-or-node": "^2.0.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-junit": "^1.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "finalhandler": "^1.1.2",
    "jsdoc": "^3.6.7",
    "jsdoc-export-default-interop": "^0.3.1",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdoc-to-markdown": "^7.1.0",
    "lines-logger": "2.1.2",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-webdriver-runner": "^0.6.3",
    "prettier": "^2.5.1",
    "selenium-webdriver": "^4.1.0",
    "serve-static": "^1.14.2",
    "sinon": "^12.0.1"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "eventemitter3": "^4.0.7",
    "google-closure-compiler": "^20211201.0.0",
    "google-closure-library": "^20211201.0.0",
    "path-browserify": "^1.0.1",
    "rambda": "^7.0.1",
    "rimraf": "^3.0.2",
    "standardized-audio-context": "^25.3.18",
    "text-encoding-shim": "^1.0.5",
    "unmute-ios-audio": "^3.3.0",
    "web-midi-api": "^2.1.3"
  }
}
